{
  "name": "momohiki",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .next out public/min-maps public/monaco-editor public/output.css next-env.d.ts",
    "dev": "concurrently \"yarn dev-css\" \"next dev\"",
    "dev-css": "postcss --watch styles/globals.css --config postcss.config.js -o public/output.css",
    "build": "yarn build-css && next build",
    "build-css": "postcss styles/globals.css --config postcss.config.js -o public/output.css",
    "export": "yarn clean && yarn build && next export",
    "start": "next start",
    "lint": "next lint",
    "fmt": "prettier --write .",
    "serve": "serve out"
  },
  "dependencies": {
    "next": "^13.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@monaco-editor/react": "^4.4.5",
    "@types/node": "18.7.18",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.4.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "8.23.1",
    "eslint-config-next": "13.0.2",
    "monaco-editor": "^0.34.0",
    "msw": "^0.47.3",
    "postcss": "^8.4.18",
    "postcss-cli": "^10.0.0",
    "prettier": "^2.7.1",
    "rehype-highlight": "^6.0.0",
    "rehype-react": "^7.1.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "serve": "^14.0.1",
    "tailwindcss": "^3.2.2",
    "typescript": "4.8.3",
    "unified": "^10.1.2",
    "zustand": "^4.1.1"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
