{
  "name": "momohiki",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn dev-css\" \"next dev\"",
    "dev-css": "postcss --watch styles/globals.css --config postcss.config.js -o public/output.css",
    "build": "yarn build-css && next build",
    "build-css": "postcss styles/globals.css --config postcss.config.js -o public/output.css",
    "export": "yarn build && next export",
    "start": "next start",
    "lint": "next lint",
    "fmt": "prettier --write .",
    "serve": "serve out"
  },
  "dependencies": {
    "next": "12.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.7.13",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "eslint": "8.23.0",
    "eslint-config-next": "12.2.5",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "prettier": "^2.7.1",
    "serve": "^14.0.1",
    "tailwindcss": "^3.1.8",
    "typescript": "4.8.2"
  }
}
