{
  "name": "momohiki",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn dev:editor\" \"yarn dev:viewer\"",
    "dev:editor": "next dev",
    "dev:viewer": "parcel viewer/template.html --dist-dir public/viewer --cache-dir /tmp/.parcel-cache",
    "build": "next build",
    "export": "next build && next export",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier . --write"
  },
  "dependencies": {
    "next": "12.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.6.2",
    "@parcel/transformer-typescript-tsc": "^2.6.2",
    "@parcel/validator-typescript": "^2.6.2",
    "@types/node": "18.6.1",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.3.0",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.3",
    "parcel": "^2.6.2",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "tailwindcss": "^3.1.6",
    "typescript": "4.7.4"
  }
}
