{
  "name": "momohiki",
  "version": "0.0.0",
  "private": true,
  "description": "Markdown Manual Editor.",
  "scripts": {
    "dev": "yarn workspace momohiki-editor run dev",
    "export": "yarn workspace momohiki-editor run export && mv packages/momohiki-editor/out ./"
  },
  "author": "Katsutoshi Fujihara",
  "license": "MIT",
  "workspaces": {
    "packages": [
      "packages/momohiki-common",
      "packages/momohiki-editor",
      "packages/momohiki-viewer"
    ],
    "nohoist": ["**/**"]
  }
}
